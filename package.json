{
  "name": "react-template",
  "version": "1.0.0",
  "description": "mysql react express sample",
  "main": "app.js",
  "author": "hazuki",
  "license": "MIT",
  "scripts": {
    "start": "set NODE_ENV=development && electron main.js",
    "start-web-dev-server": "webpack-dev-server",
    "lint": "eslint  src/**/*.{js,jsx}",
    "lint-scripts": "eslint  scripts/**/*.{js,jsx}",
    "build": "set NODE_ENV=development && webpack --progress",
    "build-scripts": "set NODE_ENV=development && webpack --config webpack-scripts.config.babel.js --progress",
    "releaseBuild": "set NODE_ENV=production && webpack --progress",
    "packageBuild": "electron-packager . react-template --platform=darwin,win32 --version=1.0.0 "
  },
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "classnames": "^2.2.5",
    "electron": "^1.4.15",
    "electron-window-state": "^4.0.1",
    "immutable": "^3.8.1",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-immutable": "^3.0.10"
  },
  "devDependencies": {
    "babel-core": "latest",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-babili": "^0.0.9",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "6.2.4",
    "css-loader": "^0.26.1",
    "electron-packager": "^8.4.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.2.0",
    "html-loader": "^0.4.4",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
